  },
  "devDependencies": {
    "ajv": "^6",
    "core-js": "3.1.4",
    "react-is": "16.8.2",
    "redoc-cli": "^0.12.3"
  },
  "dependencies": {
    "@redocly/openapi-cli": "^1.0.0-beta.78",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
